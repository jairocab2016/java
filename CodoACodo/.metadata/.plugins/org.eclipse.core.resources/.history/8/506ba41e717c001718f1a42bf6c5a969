import javax.swing.*;

public class SwingJFrame extends JFrame
{
    public SwingJFrame()
    {
        setLayout(null);
    }

    public static void main(String[] ar) 
    s{
        Formulario formulario1=new Formulario();
        formulario1.setBounds(10,20,400,300);
        formulario1.setVisible(true);
    }
}