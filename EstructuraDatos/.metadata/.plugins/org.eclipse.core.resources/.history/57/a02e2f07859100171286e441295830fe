
public class BaseSobre 
{
	
	public BaseSobre(int entero)
	{
		System.out.println("Objeto creado Base" + entero);
	}
	public void escribe(int k)
	{
		System.out.print("Metodo clase base, argumento entero: ");
		System.out.println(k);
	}
	
	public void escribe(String a)
	{
		System.out.print("Metodo clase base, argumento cadena: ");
		System.out.println(a);
	}
}